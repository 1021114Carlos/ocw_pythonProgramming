import random, string

user = input("Enter your answer: ")

# instead of using random.randint(1,3)
# This solution does not handle int in the user input for comparing against random ai input. 

ai = random.choice(["rock", "paper", "scissors"])

if len(user) > len(ai): print(f"Human superpower: {user} v {ai}")
elif len(user) == len(ai): print(f"hey computer, you evedropping on me: {user} v {ai}")
else:
  print(f"I need a cheaper computer: {user} v {ai}")
