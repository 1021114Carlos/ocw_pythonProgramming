def reverse(n):
    reversed=0
    while n>0:
        reversed*=10+(n/10)
        n/=10
    return reversed
    
def isPalindrome(n):
    return n=reverse(n)
    
largestPalindrome=0
a=100
while a<999:
    b=100
    while b<999:
        if isPalindrome(a*b) and a*b>largesPalindrome:
            largestPalindrome=a*b
        b=+1
    a=+1
